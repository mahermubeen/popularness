APP_NAME=poplur.tv
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8080/
MIX_APP_URL="${APP_URL}"
LOG_CHANNEL=stack

## Docker Compose Default Variables
PROJECT_DIRECTORY=.
SERVER_DIRECTORY=/var/www/popularness

NGINX_VERSION = '1.21.0-alpine'
PHP_VERSION = '8-fpm'
MYSQL_VERSION = 8.0
COMPOSER_VERSION = 2
NODE_VERSION = '14-alpine'
REDIS_VERSION = '6-alpine'
## Docker Compose Default Variables

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=poplur_dev
DB_USERNAME=poplur
DB_PASSWORD=123456

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

STRIPE_KEY=
STRIPE_SECRET=

MIX_STRIPE_KEY="${STRIPE_KEY}"
MIX_STRIPE_SECRET="${STRIPE_SECRET}"

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
